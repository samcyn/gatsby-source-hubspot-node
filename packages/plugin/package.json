{
  "name": "gatsby-source-hubspot-nodes",
  "description": "The `gatsby-source-hubspot-nodes` plugin seamlessly integrates your Gatsby site with HubSpot, enabling you to source data directly from HubSpot into your Gatsby application",
  "author": "samcyn <samsoniyanda@outlook.com>",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/gatsby-node.js",
  "files": [
    "./dist/*",
    "gatsby-node.js"
  ],
  "scripts": {
    "clean": "del-cli dist",
    "build": "tsc",
    "develop": "tsc --watch",
    "prepare": "npm run clean && npm run build",
    "semantic-release": "semantic-release --branches release"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "hubspot",
    "cms"
  ],
  "types": "dist/types.d.ts",
  "engines": {
    "node": ">=20.8.1"
  },
  "devDependencies": {
    "del-cli": "^5.1.0",
    "gatsby": "^5.13.3",
    "gatsby-plugin-utils": "^4.13.1",
    "semantic-release": "^23.0.6",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "gatsby": "^5.13.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samcyn/gatsby-source-hubspot-nodes.git"
  }
}
