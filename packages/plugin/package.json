{
  "name": "plugin",
  "description": "The `gatsby-source-hubspot-nodes` plugin seamlessly integrates your Gatsby site with HubSpot, enabling you to source data directly from HubSpot into your Gatsby application",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/gatsby-node.js",
  "files": [
    "./dist/*",
    "gatsby-node.js"
  ],
  "scripts": {
    "clean": "del-cli dist",
    "build": "tsc",
    "develop": "tsc --watch",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "hubspot",
    "cms"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "del-cli": "^5.1.0",
    "gatsby": "^5.13.3",
    "gatsby-plugin-utils": "^4.13.1",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "gatsby": "^5.13.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.8"
  }
}
